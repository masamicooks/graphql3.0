{
  "name": "graphqlCourse",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "NODE_ENV=development nodemon --ignore './client/' --exec babel-node server/index.js",
    "dev:client": "cd client && yarn start",
    "prod:build": "webpack --config webpack.config.js",
    "prod:setup": "NODE_ENV=production pm2 deploy production setup",
    "prod:deploy": "NODE_ENV=production pm2 deploy ecosystem.config.js production",
    "prod:serve": "NODE_ENV=production pm2 startOrRestart ecosystem.config.js"
  },
  "dependencies": {
    "apollo-server-express": "^2.13.0",
    "babel-node": "^0.0.1-security",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.4.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "morgan": "^1.10.0",
    "uuid": "^8.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
